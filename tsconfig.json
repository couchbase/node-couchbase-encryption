{
    "compilerOptions": {
       "lib": ["es2015"],
       "module": "commonjs",
       "target": "es2015",
   
       "strict": true,
       "esModuleInterop": true,
       "skipLibCheck": true,
       "forceConsistentCasingInFileNames": true,
   
       "outDir": "dist",
       "declaration": true,
     },
   
     "include": ["lib"],
     "exclude": [
      "./tests/",
      "./node_modules/",
      "./dist/"
    ],
   }